                    function changeButtonColor() {
                        var orderLines = document.getElementsByClassName("OrderItemLine");
                        var suggestedDeliveryName = !!document.querySelector('[id$="deliveryFieldId"]')?document.querySelector('[id$="deliveryFieldId"]').value:''; 
                        for(var i = 0; i < orderLines.length; i++){
                            var inventoryDeliveryName = orderLines[i].querySelector('[class$="inventoryDeliveryName"]').innerHTML.trim();
                            var color = (!!suggestedDeliveryName && suggestedDeliveryName == inventoryDeliveryName) ? 'green' : 'yellow';
                            var button = orderLines[i].querySelector('[id$="takeFromInventory"]');
                            if(!!button){
                                button.style.cssText = 'background-color: ' + color +  ' !important';
                            }
                        }

                        var inventoryLine = document.getElementsByClassName("inventorySubData");
                        for(var i = 0; i < inventoryLine.length; i++){
                            var inventoryDeliveryName = inventoryLine[i].querySelector('[class$="inventoryDeliveryCompanyName"]').innerHTML.trim();
                            var color = (!!suggestedDeliveryName && suggestedDeliveryName == inventoryDeliveryName) ? 'green' : 'yellow';
                            var button = inventoryLine[i].querySelector('[id$="takeFromInventory"]');
                            if(!!button){
                                button.style.cssText = 'background-color: ' + color +  ' !important';
                            }
                        }
                        //document.querySelector('[id$="takeFromInventory"]').style.cssText = 'background-color: ' + (document.querySelector('[id$="deliveryFieldId"]').value == initialDelivery ? 'green' : 'yellow') + ' !important';
                    }
                    
