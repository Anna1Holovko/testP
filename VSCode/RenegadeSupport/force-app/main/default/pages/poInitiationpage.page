                    // show hide subtable logic ----------------------

                    function setHideShowFunction(htmlElementCollection, isDisplayed){
                        [...htmlElementCollection].filter(elem => {
                                elem.classList = [...elem.classList].filter(item => item != (isDisplayed ? 'element-show' : 'element-hide')).join(' ');
                                elem.classList.add(isDisplayed ? 'element-hide' : 'element-show');
                            });
                    }

                    function showhide(num) {  
                        var tableRows = getElementsByClassName(num);
                        var tableRowsBody = getElementsByClassName(num+'white');
                        var isDisplayed = [...tableRows[0].classList].includes("element-show");             
                        if(!!tableRows){  
                            setHideShowFunction(tableRows, isDisplayed);
                            setHideShowFunction(tableRowsBody, isDisplayed);                  
                        }
                        
                    }  
                    
                    function showChooseInventory(className) {
                        var chooseInventoryTableElements = getElementsByClassName(className);
                        var isDisplayed = [...chooseInventoryTableElements[0].classList].includes("element-show");               
                        if(!!chooseInventoryTableElements){                                
                            setHideShowFunction(chooseInventoryTableElements, isDisplayed);
                            if(isDisplayed){
                                setHideShowFunction(getElementsByClassName('sub'+className), isDisplayed);
                            }
                        }
                    }  
