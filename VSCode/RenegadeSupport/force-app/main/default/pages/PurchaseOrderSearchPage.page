                        /*********** Search javascript code ************/
                        function searchPurchaseOrdersJS(status) 
                        {
                            // var Status=document.getElementById('Status').value;
                            var manufacturerName=document.getElementById('manufacturerName').value;
                            var name = document.getElementById('accName').value; 
                            //alert('Status '+status1);
                            if(status != null || manufacturerName != null || name != null)
                            {
                                purchaseOrderSearchController.searchPurchaseOrders(status,name,manufacturerName,
                                                                                   function(result, event)
                                                                                   {
                                                                                       if (event.status) 
                                                                                       {
                                                                                           if(result=='[]')
                                                                                           {
                                                                                               alert('No results found.');
                                                                                               document.getElementById('manufacturerName').value='';
                                                                                               document.getElementById('accName').value=''; 
                                                                                           }
                                                                                           else 
                                                                                               renderProducts(result);  
                                                                                       } 
                                                                                       else if (event.type == 'exception') 
                                                                                       {
                                                                                           alert(event.message);
                                                                                           document.getElementById("errors-js").innerHTML = event.message;
                                                                                       } else 
                                                                                       {
                                                                                           alert(event.message);
                                                                                           document.getElementById("errors-js").innerHTML = event.message;
                                                                                       }
                                                                                   }, {escape:true});
                            }
                        }
                        function getPrevPurchaseOrdersJS() 
                        {
                            var firstPayapalIdval = document.getElementById('firstPayapalId').value; 
                            var query = document.getElementById('storedquery').value;
                            purchaseOrderSearchController.getPrevPurchaseOrders(firstPayapalIdval,query, 
                                                                                function(result, event)
                                                                                {
                                                                                    if (event.status) 
                                                                                    {
                                                                                        if(result=='[]')
                                                                                        {
                                                                                            alert('No results found.');
                                                                                        }
                                                                                        else 
                                                                                            renderProducts(result);
                                                                                    } 
                                                                                    else if (event.type == 'exception') 
                                                                                    {
                                                                                        document.getElementById("errors-js").innerHTML = event.message;
                                                                                    } else 
                                                                                    {
                                                                                        document.getElementById("errors-js").innerHTML = event.message;
                                                                                    }
                                                                                }, {escape:true});        
                        }
                        function getNextPurchaseOrdersJS() 
                        {
                            var lastPayapalIdval = document.getElementById('lastPayapalId').value; 
                            var query = document.getElementById('storedquery').value;
                            // alert('query '+query+' lastPayapalIdval '+lastPayapalIdval);
                            purchaseOrderSearchController.getNextPurchaseOrders(lastPayapalIdval,query, 
                                                                                function(result, event)
                                                                                {
                                                                                    if (event.status) 
                                                                                    {
                                                                                        if(result=='[]')
                                                                                        {
                                                                                            alert('No results found.');
                                                                                        }
                                                                                        else 
                                                                                            renderProducts(result);
                                                                                    } 
                                                                                    else if (event.type == 'exception') 
                                                                                    {
                                                                                        document.getElementById("errors-js").innerHTML = event.message;
                                                                                    } else 
                                                                                    {
                                                                                        document.getElementById("errors-js").innerHTML = event.message;
                                                                                    }
                                                                                }, {escape:true});        
                        }
                        
                        
                        /************ Search javascript code ************/
                        /*************** Search Button Enter Functionality *********/
                        function uniKeyCode(event) {
                            if(event.keyCode == 13){
                                //   alert('event.keyCode is ----'+event.keyCode);
                                event.preventDefault();
                                searchPurchaseOrdersJS(document.getElementById('j_id0:formId:j_id70:Details').value);
                                return false;
                            }
                        }
                        /*************** Search Button Enter Functionality *********/ 
                        
